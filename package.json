{
  "name": "sandbox",
  "version": "0.0.0",
  "author": "vlad.dancer @vladdancer",
  "bin": {
    "sandbox": "./bin/run"
  },
  "bugs": "https://github.com/vladdancer/sandbox/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/execa": "^2.0.0",
    "await-to-js": "^2.1.1",
    "chalk": "^3.0.0",
    "enquirer": "^2.3.4",
    "execa": "^4.0.0",
    "fs-extra": "^8.1.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^7.0.4",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "js-yaml": "^3.13.1",
    "listr": "^0.14.3",
    "replace-in-file": "^5.0.2",
    "shortid": "^2.2.15",
    "signale": "^1.4.0",
    "sudo-prompt": "^9.1.1",
    "swig-templates": "^2.0.3",
    "tslib": "^1",
    "v8n": "^1.5.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/fs-extra": "^8.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/js-yaml": "^3.12.2",
    "@types/listr": "^0.14.2",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/shortid": "^0.0.29",
    "@types/signale": "^1.2.1",
    "chai": "^4",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/vladdancer/sandbox",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sandbox",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "macos": {
      "identifier": "pro.xspace.sandboxcli"
    },
    "hooks": {
      "init": "./lib/hooks/init/set-global-config"
    }
  },
  "repository": "vladdancer/sandbox",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
